        <script>
          jQuery(document).ready(function(){
            jQuery(".prev").click(function(){
              moveRight();
            });
            jQuery(".next").click(function(){
              moveLeft();
            });

            function moveLeft(){
              if(jQuery('.slides .active').next('li').length) {
                jQuery('.slides .active').next().addClass('active').prev().removeClass('active');
              }else{
                jQuery('.slides li').removeClass('active');
                jQuery('.slides li:first-child').addClass('active');
              }
                jQuery(".slides").scrollCenter(".active", 300);
                var currentImg = jQuery('.slides .slide-item.active').attr('data-thumb');
                jQuery('div#sliderImage img').attr('src',currentImg);
                jQuery('div#sliderImage').show();
                jQuery('.featured-product .product__media-item').addClass('hide');
            }
            function moveRight(){
              if(jQuery('.slides .active').prev('li').length) {
                  jQuery('.slides .active').prev().addClass('active').next().removeClass('active');
              }else{
                  jQuery('.slides li').removeClass('active');
                  jQuery('.slides li:last-child').addClass('active');
              }
              jQuery(".slides").scrollCenter(".active", 300);
              var currentImg = jQuery('.slides .slide-item.active').attr('data-thumb');
                jQuery('div#sliderImage img').attr('src',currentImg);
                jQuery('div#sliderImage').show();
                jQuery('.featured-product .product__media-item').addClass('hide');
            }

            jQuery.fn.scrollCenter = function(elem, speed) {
              // alert('aaa');
              var active = jQuery(this).find(elem); // find the active element
              var activeWidth = active.width() / 2;
              var pos = active.position().left + activeWidth; //get left position of active li + center position
              var elpos = jQuery(this).scrollLeft(); // get current scroll position
              var elW = jQuery(this).width(); //get div width
              pos = pos + elpos - elW / 2; // for center position if you want adjust then change this
              jQuery(this).animate({
                scrollLeft: pos
              }, speed == undefined ? 1000 : speed);
              return this;
            };

            $("div#product-flexslider-custom ul.slides li.slide-item").on('click',function(e){
                jQuery('#product-flexslider-custom .slides li').removeClass('active');
                jQuery(this).addClass('active');
                jQuery(".slides").scrollCenter(".active", 300);
              var currentImg = jQuery('.slides .slide-item.active').attr('data-thumb');
                jQuery('div#sliderImage img').attr('src',currentImg);
                jQuery('div#sliderImage').show();
                jQuery('.featured-product .product__media-item').addClass('hide');
            });

            $(".product__info-container variant-radios fieldset").on('click',function(e){
                jQuery('.featured-product .product__media-item').removeClass('hide');
                jQuery('div#sliderImage').hide();
            });

          });
        </script>
